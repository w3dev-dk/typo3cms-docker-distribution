{
    "name": "w3dev/typo3cms-docker-distribution",
    "description": "This is the source code for TYPO3 CMS & docker distribution package",
    "license": "gpl2",
    "type": "project",
    "minimum-stability": "stable",
    "authors": [
        {
            "name": "Lasse Enoe Barslund",
            "email": "lasse_enoe@hotmail.com",
            "role": "Developer"
        }
    ],
    "repositories": [
        { "type": "path", "url": "packages/*/" },
        { "type": "composer", "url": "https://composer.typo3.org/" }
    ],
    "require": {
        "typo3/cms": "^8.7",
        "typo3/cms-about": "^8.7",
        "typo3/cms-belog": "^8.7",
        "typo3/cms-beuser": "^8.7",
        "typo3/cms-context-help": "^8.7",
        "typo3/cms-documentation": "^8.7",
        "typo3/cms-felogin": "^8.7",
        "typo3/cms-fluid-styled-content": "^8.7",
        "typo3/cms-form": "^8.7",
        "typo3/cms-func": "^8.7",
        "typo3/cms-impexp": "^8.7",
        "typo3/cms-info": "^8.7",
        "typo3/cms-info-pagetsconfig": "^8.7",
        "typo3/cms-lowlevel": "^8.7",
        "typo3/cms-opendocs": "^8.7",
        "typo3/cms-recycler": "^8.7",
        "typo3/cms-reports": "^8.7",
        "typo3/cms-rsaauth": "^8.7",
        "typo3/cms-rte-ckeditor": "^8.7",
        "typo3/cms-setup": "^8.7",
        "typo3/cms-sys-action": "^8.7",
        "typo3/cms-sys-note": "^8.7",
        "typo3/cms-t3editor": "^8.7",
        "typo3/cms-taskcenter": "^8.7",
        "typo3/cms-tstemplate": "^8.7",
        "typo3/cms-version": "^8.7",
        "typo3/cms-wizard-crpages": "^8.7",
        "typo3/cms-wizard-sortpages": "^8.7",
        "typo3/cms-workspaces": "^8.7",
        "helhum/typo3-console": "^4.6",
        "dmitryd/typo3-realurl": "2.2.*"
    },
    "scripts": {
        "package-states": [
            "@php vendor/helhum/typo3-console/Scripts/typo3cms install:generatepackagestates"
        ],
        "folder-structure": [
            "@php vendor/helhum/typo3-console/Scripts/typo3cms install:fixfolderstructure"
        ],
        "ext-setup": [
            "@php vendor/helhum/typo3-console/Scripts/typo3cms install:extensionsetupifpossible"
        ],
        "post-autoload-dump": [
            "@package-states",
            "@folder-structure",
            "@ext-setup",
            "/bin/bash fix-permissions.sh"
        ]
    },
    "extra": {
        "helhum/typo3-console": {
            "install-extension-dummy": false
        },
        "typo3/cms": {
            "cms-package-dir": "{$vendor-dir}/typo3/cms",
            "web-dir": "html"
        }
    }
}
